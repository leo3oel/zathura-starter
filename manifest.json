{
    "required_api_version": "^2.0.0",
    "name": "Zathura Starter",
    "description": "Open a PDF File in Zathura",
    "developer_name": "Leonhard Schraut",
    "icon": "images/icon.png",
    "options": {
      "query_debounce": 0.1
    },
    "preferences": [
      {
        "id": "zat_kw",
        "type": "keyword",
        "name": "Start Keyword",
        "description": "Keyword to open extension",
        "default_value": "zat"
      },
      {
        "id": "start_path",
        "type": "text",
        "name": "Start Path",
        "description": "Path to start PDF search; Input without '/' at the end e.g. /home",
        "default_value": "~"
      }
    ]
}